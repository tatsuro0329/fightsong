<header>
  <div class="left">
    <%= link_to image_tag("画像のファイル名"), root_path %>
  </div>

  <div class="right">
    <% if user_signed_in? %>
      <%= link_to current_user.name, "#" %>
      <%= link_to "ログアウト", user_destroy_path, method: :delete %>
    <% else %>
      <%= link_to "ログイン", user_session_path %>
      <%= link_to "新規登録", new_user_registration_path %>
    <% end %>
  </div>
</header>
